{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "removeComments": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "components/*": ["src/components/*"]
    },
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "preserveSymlinks": true
  }
}
